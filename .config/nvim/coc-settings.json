{
  "coc.preferences.floatActions": false,
  "diagnostic.enableMessage": "jump",
  "diagnostic.checkCurrentLine": true,
  "signature.hideOnTextChange": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "▪  ",
  "diagnostic.enableSign": false,
  "diagnostic.errorSign": "▎",
  "diagnostic.warningSign": "▎",
  "diagnostic.hintSign": "▎",
  "diagnostic.infoSign": "▎",
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "javascriptreact", "typescriptreact"],
  "javascript.format.enabled": false,
  "typescript.format.enabled": false,
  "typescript.updateImportsOnFileMove.enable": false,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.bracketEnterImprove": false,
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": ["-c", "/home/phelipe/.config/efm-langserver/config.yaml"],
      "filetypes": ["sh", "yaml", "lua", "python", "javascript", "typescript", "javascriptreact", "typescriptreact"],
      "formatterPriority": 100
    }
  },
  "workspace.workspaceFolderCheckCwd": true
}
